    "@types/koa": "2.13.10",
    "@types/node": "20.8.9",
    "@typescript-eslint/eslint-plugin": "6.9.0",
    "@typescript-eslint/parser": "6.9.0",
    "@vitest/coverage-v8": "0.34.6",
    "del-cli": "5.1.0",
    "eslint": "8.52.0",
    "husky": "8.0.3",
    "lint-staged": "15.0.2",
    "prettier": "3.0.3",
    "tsx": "3.14.0",
    "typescript": "5.2.2",
    "vitest": "0.34.6"
  },
  "dependencies": {
    "@feathersjs/feathers": "5.0.11",
    "@feathersjs/koa": "5.0.11",
    "@feathersjs/socketio": "5.0.11",
    "@hapi/hapi": "21.3.2",
    "@types/express": "4.17.20",
    "express": "4.18.2",
    "fastify": "4.24.3",
    "koa": "2.14.2",
    "restana": "4.9.7"
